<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginNew"
    android:background="@drawable/fondo2">

    <TextView
        android:id="@+id/beta"
        android:layout_width="282dp"
        android:layout_height="21dp"
        android:textAlignment="textEnd"
        style="@style/etiquetas2"
        android:textSize="15sp"
        android:text="BETA V.1.02.0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textNewName"
        style="@style/etiquetas2"
        android:layout_width="333dp"
        android:layout_height="74dp"
        android:hint="@string/hint"
        android:text="@string/doble_clic"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usu"
        app:layout_constraintVertical_bias="0.033"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/usu"
        style="@style/etiquetas2"
        android:layout_width="410dp"
        android:layout_height="62dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="@string/nombre"
        android:textAlignment="center"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aviso"
        app:layout_constraintVertical_bias="0.034"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/aviso"
        style="@style/etiquetas2"
        android:layout_width="411dp"
        android:layout_height="115dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="@string/aviso_se_guardara_la_coordenada_de_su_posicion_actual"
        android:textAlignment="center"
        android:textColor="#CC0A0A"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/usu2"
        style="@style/etiquetas2"
        android:layout_width="402dp"
        android:layout_height="67dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="21dp"
        android:text="@string/telefono_correo"
        android:textAlignment="center"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNewName"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textNewMail"
        style="@style/etiquetas2"
        android:layout_width="333dp"
        android:layout_height="74dp"
        android:hint="@string/escribe_aqui"
        android:text=""
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usu2"
        app:layout_constraintVertical_bias="0.061"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonCrearNew"
        style="@style/etiquetas"
        android:layout_width="232dp"
        android:layout_height="84dp"
        android:layout_alignParentEnd="true"
        android:background="@drawable/btnoscuro2"
        android:onClick="Crear"
        android:text="@string/generar_coordenadas"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNewMail"
        app:layout_constraintVertical_bias="0.195"
        tools:ignore="MissingConstraints,OnClick" />

    <Button
        android:id="@+id/buttonLogin2"
        style="@style/etiquetas"
        android:layout_width="127dp"
        android:layout_height="62dp"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="36dp"
        android:background="@drawable/btnoscuro2"
        android:onClick="volver"
        android:text="@string/atras"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCrearNew"
        app:layout_constraintVertical_bias="0.342"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>